<!DOCTYPE html>
<html>
<head>
  <title>Twitter stocks</title>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag  '//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag  '//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-default navbar-static-top navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">Twitter Stocks</a>
    </div>
    <ul class="nav navbar-nav">
      <li class="active">
        <a href="/"><img src="<%= asset_path('lang-logo.png') %>">Home</a>
      </li>
      <li class="navbar-right">
        <a href="#"><span class="glyphicon glyphicon-user"></span>Calendar</a>
      </li>
      <li class="navbar-right">
        <a href="#"><span class="glyphicon glyphicon-info-sign"></span>Stocks</a>
      </li>
    </ul>
    <ul class="nav navbar-nav navbar-right">
      <li class="navbar-right">
        <a href="#"><span class="glyphicon glyphicon-calendar"></span>Calendar</a>
      </li>
      <li class="navbar-right">
        <a href="#"><span class="glyphicon glyphicon-stats"></span>Stocks</a>
      </li>
    </ul>
  </div>
</nav>
<div id="content">
<%= yield %>
</div>
</body>
</html>
